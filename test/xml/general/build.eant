<?xml version="1.0"?>

<project name="eiffel" default="help">

	<description>
		system: "Gobo Eiffel XML Library Tester"
		copyright: "Copyright (c) 2002, Eric Bezault and others"
		license: "MIT License"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${GOBO}/misc/test.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>


<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="xgeneral"/>
		<set name="library" value="xml"/>
		<set name="system_dir" value="${GOBO}/test/${library}/general"/>
	</target>

</project>
