<?xml version="1.0"?>

<project name="eiffel" default="help">

	<description>
		system: "Gobo Eiffel XML Library Unit Tester"
		copyright: "Copyright (c) 2002-2016, Eric Bezault and others"
		license: "MIT License"
	</description>

	<inherit>
		<parent location="${GOBO}/library/common/config/test.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>

<!-- Implementation -->

	<target name="init_system" export="NONE">
		<set name="system" value="xeiffel"/>
		<set name="library" value="xml"/>
		<set name="system_dir" value="${GOBO}/library/${library}/test/unit/eiffel"/>
	</target>

</project>
