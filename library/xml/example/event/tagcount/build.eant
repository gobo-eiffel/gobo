<?xml version="1.0"?>

<project name="tagcount" default="help" >

	<description>
		system: "Tutorial event example"
		copyright: "Copyright (c) 2003-2016, Andreas Leitner and others"
		license: "MIT License"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${GOBO}/library/common/config/eiffel.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>

<!-- Implementation -->

	<target name="init_system" export="NONE">
		<set name="system" value="tagcount"/>
		<set name="library" value="xml"/>
		<set name="system_dir" value="${GOBO}/library/${library}/example/event/${system}"/>
	</target>

</project>
