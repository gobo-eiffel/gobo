				Result := agent_open_operands.item (i)
			else
				from
					j := agent_open_operands.count + 1
					create Result.make ("a" + j.out)
					Result.set_agent_open_operand (True)
					Result.set_seed (j)
					agent_open_operands.force_last (Result)
					j := j + 1
