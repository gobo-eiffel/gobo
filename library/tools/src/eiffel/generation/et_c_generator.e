					header_file.put_character ('(')
					header_file.put_string (c_void)
					header_file.put_character (')')
					header_file.put_character (';')
					header_file.put_new_line
					current_file.put_string (c_void)
					current_file.put_character (' ')
					current_file.put_string (c_ge_init_const)
