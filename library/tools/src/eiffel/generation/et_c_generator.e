feature {NONE} -- Inlined operands

	new_inlined_operand (a_expression: ET_EXPRESSION; a_context: ET_DYNAMIC_CALL_CONTEXT): ET_DYNAMIC_INLINED_EXPRESSION
			-- New inlined operand
		require
			a_expression_not_void: a_expression /= Void
			a_context_not_void: a_context /= Void
		local
			l_old_feature: like current_feature
			l_dynamic_type_set: ET_DYNAMIC_TYPE_SET
			l_od_current_index: INTEGER
			l_old_index_offset: INTEGER
		do
			l_old_feature := current_feature
			l_od_current_index := current_index
			l_old_index_offset := index_offset
			current_feature := a_context.current_feature
			current_type := current_feature.target_type
			current_dynamic_type_sets := current_feature.dynamic_type_sets
			current_index := current_feature.current_index
			if current_feature /= l_old_feature then
				index_offset := 0
			end
			l_dynamic_type_set := dynamic_type_set (a_expression)
			current_feature := l_old_feature
			current_type := current_feature.target_type
			current_dynamic_type_sets := current_feature.dynamic_type_sets
			index_offset := l_old_index_offset
			if unused_inlined_operands.is_empty then
				create Result.make (a_expression, l_dynamic_type_set, a_context)
			else
				Result := unused_inlined_operands.last
				unused_inlined_operands.remove_last
				Result.set_expression (a_expression)
				Result.set_dynamic_type_set (l_dynamic_type_set)
				Result.set_context (a_context)
			end
			used_inlined_operands.force_last (Result)
			if attached {ET_DYNAMIC_INLINED_EXPRESSION} a_expression as l_inlined_expression then
				Result.set_expression (l_inlined_expression.expression)
				Result.set_context (l_inlined_expression.context)
			end
		ensure
			new_inlined_operand_not_void: Result /= Void
			expression_set: not attached {ET_DYNAMIC_INLINED_EXPRESSION} a_expression implies (Result.expression = a_expression)
			context_set: not attached {ET_DYNAMIC_INLINED_EXPRESSION} a_expression implies (Result.context = a_context)
			expression_kept: attached {ET_DYNAMIC_INLINED_EXPRESSION} a_expression as l_inlined_expression implies (Result.expression = l_inlined_expression.expression)
			context_kept: attached {ET_DYNAMIC_INLINED_EXPRESSION} a_expression as l_inlined_expression implies (Result.context = l_inlined_expression.context)
			dynamic_type_set_set: Result.dynamic_type_set = dynamic_type_set (a_expression)
		end

	free_inlined_operands
			-- Free all inlined operand objects so that they can be reused.
		local
			i, nb: INTEGER
			l_inlined_operand: ET_DYNAMIC_INLINED_EXPRESSION
		do
			nb := used_inlined_operands.count
			from i := 1 until i > nb loop
				l_inlined_operand := used_inlined_operands.item (i)
				l_inlined_operand.set_expression (tokens.current_keyword)
				l_inlined_operand.set_dynamic_type_set (dummy_feature.target_type)
				unused_inlined_operands.force_last (l_inlined_operand)
				i := i + 1
			end
			used_inlined_operands.wipe_out
		end

