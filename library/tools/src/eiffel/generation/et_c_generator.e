					current_file.put_character ('1')
					current_file.put_character (';')
					current_file.put_character (' ')
					print_temp_name (l_i_temp, current_file)
					print_greater_than_or_equal
					current_file.put_character ('0')
					current_file.put_character (';')
					current_file.put_character (' ')
					print_temp_name (l_i_temp, current_file)
					current_file.put_character ('-')
					current_file.put_character ('-')
