			a_file_not_void: a_file /= Void
			a_file_open_write: a_file.is_open_write
		do
			if not a_type.is_expanded or else a_type.is_basic then
				a_file.put_character ('0')
			else
				print_default_object_value (a_type, a_file)
			end
		end

