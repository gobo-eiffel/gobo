<?xml version="1.0"?>

<project name="concat2" default="help">

	<description>
		system: "concat2 getest example"
		copyright: "Copyright (c) 2001-2016, Eric Bezault and others"
		license: "MIT License"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${GOBO}/tool/geant/config/test.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>


<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="xconcat2"/>
		<set name="library" value="test"/>
		<set name="example" value="concat2"/>
		<set name="system_dir" value="${GOBO}/library/${library}/example/${example}"/>
	</target>

</project>
