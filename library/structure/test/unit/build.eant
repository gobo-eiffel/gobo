<?xml version="1.0"?>

<project name="structure" default="help">

	<description>
		system: "Gobo Eiffel Structure Library Unit Tester"
		copyright: "Copyright (c) 2001-2016, Eric Bezault and others"
		license: "MIT License"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${GOBO}/tool/geant/config/test.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>

<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="xstructure"/>
		<set name="library" value="structure"/>
		<set name="system_dir" value="${GOBO}/library/${library}/test/unit"/>
	</target>

</project>
