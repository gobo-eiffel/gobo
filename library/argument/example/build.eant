<?xml version="1.0"?>

<project name="argument_example" default="help">

	<description>
		system: "Argument parsing example"
		copyright: "Copyright (c) 2006-2016, Bernd Schoeller and others"
		license: "MIT License"
	</description>

	<inherit>
		<parent location="${GOBO}/library/common/config/eiffel.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>

<!-- Implementation -->

	<target name="init_system" export="NONE">
		<set name="system" value="ap_example"/>
		<set name="library" value="argument"/>
		<set name="system_dir" value="${GOBO}/library/${library}/example"/>
	</target>

</project>
