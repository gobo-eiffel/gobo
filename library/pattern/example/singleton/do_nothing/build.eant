<?xml version="1.0"?>

<project name="do_nothing" default="help">

	<description>
		system: "Example for the Singleton pattern with does nothing"
		copyright: "Copyright (c) 2003-2016, Eric Bezault and others"
		license: "MIT License"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${GOBO}/tool/geant/config/eiffel.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>


<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="do_nothing"/>
		<set name="library" value="pattern"/>
		<set name="system_dir" value="${GOBO}/library/${library}/example/singleton/${system}"/>
	</target>

</project>
