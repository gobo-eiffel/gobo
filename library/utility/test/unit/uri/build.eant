<?xml version="1.0"?>

<project name="uri" default="help">

	<description>
		system: "Gobo Eiffel URI Library Unit Tester"
		copyright: "Copyright (c) 2004-2016, Eric Bezault and others"
		license: "MIT License"
		date: "$Date$"
		revision: "$Revision$"
	</description>

	<inherit>
		<parent location="${GOBO}/misc/test.eant">
			<redefine target="init_system"/>
		</parent>
	</inherit>


<!-- Implementation -->

	<target name="init_system">
		<set name="system" value="xuri"/>
		<set name="library" value="utility"/>
		<set name="system_dir" value="${GOBO}/library/${library}/test/unit/uri"/>
	</target>

</project>
